  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "simple-git": "^2.31.0",
    "web-ext": "^5.4.1",
    "yafs": "^1.7.0",
    "zarro": "^1.82.0"
  }
